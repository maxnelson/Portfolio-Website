{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "imports": {
    "#src/*": "./src/*",
    "#root/*": "./*"
  },
  "scripts": {
    "dev": "node --env-file=.env -- node_modules/vite/bin/vite.js",
    "build": "node --env-file=.env -- node_modules/typescript/bin/tsc && node node_modules/vite/bin/vite.js build",
    "uploadResume": "node --env-file=.env -- src/scripts/uploadResumeData/uploadResumeData.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "node server.js",
    "start_dev": "nodemon server.js"
  },
  "dependencies": {
    "@firebase/storage": "^0.12.2",
    "@react-pdf/renderer": "^3.1.15",
    "@tanstack/react-query": "^5.21.7",
    "buffer": "^6.0.3",
    "express": "^4.18.2",
    "firebase": "^10.7.1",
    "luxon": "^3.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1"
  },
  "devDependencies": {
    "@modularmoon/classycss": "^1.0.19",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "firebase-admin": "^12.4.0",
    "postcss": "^8.4.32",
    "postcss-import": "^15.1.0",
    "prettier": "^3.1.1",
    "sass": "^1.69.7",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
